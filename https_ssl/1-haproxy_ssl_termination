global
    tune.ssl.default-dh-param 2048

defaults
    mode http
    timeout client 15s
    timeout connect 10s
    timeout server 15s
    timeout http-request 10s

frontend clickviral-tech-frontend
    bind *:80
    bind *:443 ssl crt /etc/letsencrypt/live/www.desmonddarlington.tech/fullchain.pem key /etc/letsencrypt/live/www.desmonddarlington.tech/privkey.pem
    redirect scheme https code 301 if !{ ssl_fc }
    default_backend clickviral-tech-backend

backend clickviral-tech-backend
    balance roundrobin
    server 6168-web-01 54.224.21.202:80 check
    server 6168-web-02 100.26.146.29:80 check
